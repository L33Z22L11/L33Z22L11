{
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "Iosevka, JetBrains Mono, JetBrainsMono Nerd Font, Sarasa Gothic SC, PingFang SC, HarmonyOS Sans SC, Consolas",
    "editor.fontLigatures": true,
    "editor.fontSize": 17,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "boundary",
    "editor.smoothScrolling": true,
    "editor.wordWrap": "on",
    "workbench.list.smoothScrolling": true,
    "workbench.editor.wrapTabs": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: Chromium, IndentWidth: 4}",
    "settingsSync.ignoredSettings": [
        "C_Cpp.intelliSenseEngine"
    ],
    "settingsSync.ignoredExtensions": [
        "llvm-vs-code-extensions.vscode-clangd"
    ],
    "csscomb.preset": "csscomb",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "material-product-icons",
    "[python]": {
        "editor.formatOnType": true
    },
    "redhat.telemetry.enabled": true,
    "workbench.colorTheme": "One Dark Pro"
}